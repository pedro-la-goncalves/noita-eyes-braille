#!/usr/bin/env node
const fs = require('fs')
const path = require('path')

const text = fs.readFileSync(path.resolve(__dirname, '../dots.txt'), 'utf8')
const regex = (/^U\+(.{4}).+DOTS-(\d+)/gm)

let match = regex.exec(text)
while (match) {
  const [, unicode, dots] = match
  const code = `\\u${unicode}`
  const char = eval(`'${code}'`)
  console.log(unicode, dots, `\\u${unicode}`, char)
  match = regex.exec(text)
}
// U+2800	BRAILLE PATTERN BLANK (U+2800)	â €	code2000









// https://en.wikipedia.org/wiki/Braille_pattern_dots-236
